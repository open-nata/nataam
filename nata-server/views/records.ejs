<% include partials/header %>

<div id="records">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="centered">任务列表</h1>
                <br>
                <table class="table">
                    <thead>
                    <tr>
                        <th>
                            序号
                        </th>
                        <th>
                            设备ID
                        </th>
                        <th>
                            待测应用
                        </th>
                        <th>
                            Package名
                        </th>
                        <th>
                            Activity名
                        </th>
                        <th>
                            Action数量
                        </th>
                        <th>
                            算法
                        </th>
                        <th>
                            状态
                        </th>
                        <th>
                            创建时间
                        </th>
                        <th>
                            结束时间
                        </th>
                    </tr>
                    </thead>
                    <tbody>
                     <% records.forEach(function(record, index) { %>
                        <tr>
                            <td>
                                <%= index %>
                            </td>
                            <td>
                                <%= record.device_id%>
                            </td>
                            <td>
                                <%= record.app_name %>
                            </td>
                            <td>
                                <%= record.package_name %>
                            </td>
                            <td>
                                <%= record.activity_name %>
                            </td>
                            <td>
                                <%= record.action_count %>
                            </td>
                            <td>
                                <%= record.algorithm %>
                            </td>
                            <td>
                                <%= record.status %>
                            </td>
                            <td>
                                <%= record.create_at %>
                            </td>
                            <td>
                                <%= record.end_at %>
                            </td>
                        </tr>
                        <% }); %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

<% include partials/footer.ejs %>